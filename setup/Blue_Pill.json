{
  "target": "Blue_Pill",
  "board": "Blue-Pill",
  "backend": "arduino",
  "cc": {
    "name": "arduino_cli",
    "linux_path": "arduino-cli",
    "windows_path": "arduino-cli.exe",
    "flags": "compile -b STMicroelectronics:stm32:Blues -v --output-dir @{file_dir_name}/build"
  },
  "flasher": {
    "name": "arduino_cli",
    "linux_path": "arduino-cli",
    "windows_path": "arduino-cli.exe",
    "flags": "upload @{file_dir_name} -p @{target} -b STMicroelectronics:stm32:Blues"
  },
  "api_paths": ["st/blue_pill", "arduino/core", "arduino/uart", "arduino/i2c"],
  "v_defines": [],
  "default_cpu_freq": 72000000,
  "default_string_len": 50,
  "compiler_setup_path": "setup/arduino32.json"
}

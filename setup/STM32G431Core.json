{
    "target": "STM32G431Core",
    "board": "STM32G431CoreBoard",
    "backend": "arduino",
    "cc": {
        "name": "arduino-cli",
        "linux_path": "arduino-cli",
        "windows_path": "arduino-cli.exe",
        "flags": "compile -b STMicroelectronics:stm32:GenG4:pnum=GENERIC_G431C6UX -v --output-dir @{file_dir_name}/build"
    },
    "flasher": {
        "name": "arduino-cli",
        "linux_path": "arduino-cli",
        "windows_path": "arduino-cli.exe",
        "flags": "upload @{file_dir_name} -p @{target} -b STMicroelectronics:stm32:GenG4:pnum=GENERIC_G431C6UX"
    },
    "api_paths": [
        "WeAct_Studio/STM32G431Core",
        "Arduino/arduino-generic",
        "Arduino/arduino-generic-UART",
        "Arduino/arduino-generic-i2c"
    ],
    "v_defines": [],
    "default_cpu_freq": 170000000,
    "default_string_len": 50,
    "compiler_setup_path": "setup/arduino32.json"
}
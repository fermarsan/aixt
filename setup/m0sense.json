  "target": "m0sense",
  "board": "Sipeed M0sense",
  "backend": "c",
  "cc": {
    "name": "gcc",
    "linux_path": "gcc",
    "windows_path": "c:\\v\\thirdparty\\tcc\\tcc.exe",

{
    "target": "RP-Pico",
    "board": "Raspberry Pi Pico",
    "backend": "arduino",
    "cc": {
        "name": "arduino-cli",
        "linux_path": "arduino-cli",
        "windows_path": "arduino-cli.exe",
        "flags": "compile -b arduino:mbed_rp2040:pico -v --output-dir @{file_dir_name}/build"
    },
    "flasher": {
        "name": "arduino-cli",
        "linux_path": "arduino-cli",
        "windows_path": "arduino-cli.exe",
        "flags": "upload @{file_dir_name} -p @{target} -b arduino:mbed_rp2040:pico"
    },
    "api_paths": [
        "Raspberry-Pi/RP-Pico",
        "Arduino/arduino-generic",
        "Arduino/arduino-generic-UART",
        "Arduino/arduino-generic-UART1",
        "Arduino/arduino-generic-UART2",
        "Arduino/arduino-generic-i2c"
    ],
    "v_defines": [],
    "default_cpu_freq": 125000000,
    "default_string_len": 50,
    "compiler_setup_path": "setup/arduino32.json"
}
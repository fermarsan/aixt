{
    "port": "MH-ATtiny88",
    "board": "MH-ATtiny88",
    "backend": "arduino",
    "cc": {
        "linux_path": "arduino-cli",
        "windows_path": "arduino-cli.exe",
        "flags": "compile -b --fqbn MH-ET.menu.avr.ATTiny88:avr:attiny88 -v --output-dir @{file_dir_name}/build"
    },
    "flasher": {
        "linux_path": "arduino-cli",
        "windows_path": "arduino-cli.exe",
        "flags": "upload @{file_dir_name} -p @{target} -b --fqbn MH-ET.menu.avr.ATTiny88:avr:attiny88"
    },
    "api_paths": [
        "Atmel/MH-ATtiny88",
        "Arduino/arduino-AVR",
        "Arduino/arduino-generic",
        "Arduino/arduino-generic-UART",
        "Arduino/arduino-generic-i2c"
    ],
    "v_defines": [],
    "default_cpu_freq": 16000000,
    "default_string_len": 50,
    "compiler_setup_path": "setup/arduino8.json"
}
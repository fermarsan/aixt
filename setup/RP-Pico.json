{
    "port": "RP-Pico",
    "board": "Raspberry Pi Pico",
    "backend": "arduino",
    "cc_linux": "arduino-cli",
    "cc_windows": "arduino-cli.exe",
    "cc_make_flags": "compile -b arduino:mbed_rp2040:pico -v --output-dir @{file_dir_name}/build",
    "flasher_linux": "arduino-cli",
    "flasher_windows": "arduino-cli.exe",
    "flasher_flags": "upload @{file_dir_name} -p @{port} -b arduino:mbed_rp2040:pico",
    "api_paths": [
        "Raspberry-Pi/RP-Pico",
        "Arduino/arduino-generic",
        "Arduino/arduino-generic-UART",
        "Arduino/arduino-generic-UART1",
        "Arduino/arduino-generic-UART2",
        "Arduino/arduino-generic-i2c"
    ],
    "v_defines": [],
    "default_cpu_freq": 125000000,
    "default_string_len": 50,
    "compiler_setup_path": "setup/arduino32.json"
}
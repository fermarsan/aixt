{
  "target": "t_watch_2020",
  "board": "ESP32-WROOM-32",
  "backend": "arduino",
  "cc": {
    "name": "arduino_cli",
    "path": "arduino-cli",
    "args": "compile -b esp32:esp32:esp32 -v --output-dir @{file_dir_name}/build"
  },
  "flasher": {
    "name": "arduino_cli",
    "path": "arduino-cli",
    "args": "upload @{file_dir_name} -p @{port} -b esp32:esp32:esp32"
  },
  "api_paths": [
    "lilygo/t_watch_2020",
    "arduino/esp32",
    "arduino/core",
    "arduino/uart",
    "arduino/i2c"
  ],
  "v_defines": [],
  "default_cpu_freq": 240000000,
  "default_string_len": 50,
  "compiler_setup_path": "setup/arduino32.json"
}
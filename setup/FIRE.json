{
    "target": "FIRE",
    "board": "M5Stack FIRE",
    "backend": "arduino",
    "cc": {
        "name": "arduino-cli",
        "linux_path": "arduino-cli",
        "windows_path": "arduino-cli.exe",
        "flags": "compile -b m5stack:esp32:m5stack_fire -v --output-dir @{file_dir_name}/build"
    },
    "flasher": {
        "name": "arduino-cli",
        "linux_path": "arduino-cli",
        "windows_path": "arduino-cli.exe",
        "flags": "upload @{file_dir_name} -p @{target} -b m5stack:esp32:m5stack_fire"
    },
    "api_paths": [
        "M5Stack/FIRE",
        "M5Stack/M5Stack-generic",
        "Arduino/arduino-ESP32",
        "Arduino/arduino-generic",
        "Arduino/arduino-generic-UART",
        "Arduino/arduino-generic-i2c"
    ],
    "v_defines": [],
    "default_cpu_freq": 240000000,
    "default_string_len": 50,
    "compiler_setup_path": "setup/arduino32.json"
}
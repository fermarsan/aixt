{
    "port": "XIAO-SAMD21",
    "board": "Seeed Studio XIAO SAMD21",
    "backend": "arduino",
    "cc": {
        "linux_path": "arduino-cli",
        "windows_path": "arduino-cli.exe",
        "flags": "compile -b arduino:samd:nano_33_iot -v --output-dir @{file_dir_name}/build"
    },
    "flasher": {
        "linux_path": "arduino-cli",
        "windows_path": "arduino-cli.exe",
        "flags": "upload @{file_dir_name} -p @{target} -b arduino:samd:nano_33_iot"
    },
    "api_paths": [
        "Seeed-Studio/XIAO-SAMD21",
        "Arduino/arduino-generic",
        "Arduino/arduino-generic-UART",
        "Arduino/arduino-generic-i2c"
    ],
    "v_defines": [],
    "default_cpu_freq": 48000000,
    "default_string_len": 50,
    "compiler_setup_path": "setup/arduino32.json"
}
{
  "target": "rp_pico",
  "board": "Raspberry Pi Pico",
  "backend": "arduino",
  "cc": {
    "name": "arduino_cli",
    "linux_path": "arduino-cli",
    "windows_path": "arduino-cli.exe",
    "flags": "compile -b arduino:mbed_rp2040:pico -v --output-dir @{file_dir_name}/build"
  },
  "flasher": {
    "name": "arduino_cli",
    "linux_path": "arduino-cli",
    "windows_path": "arduino-cli.exe",
    "flags": "upload @{file_dir_name} -p @{port} -b arduino:mbed_rp2040:pico"
  },
  "api_paths": [
    "raspberry-pi/pico",
    "arduino/core",
    "arduino/uart",
    "arduino/arduino_uart1",
    "arduino/arduino_uart2",
    "arduino/i2c"
  ],
  "v_defines": [],
  "default_cpu_freq": 125000000,
  "default_string_len": 50,
  "compiler_setup_path": "setup/arduino32.json"
}
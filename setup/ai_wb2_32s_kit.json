{
  "target": "ai_thinker",
  "board": "Ai_Thinker",
  "backend": "arduino",
  "cc": {
    "name": "arduino_cli",
    "linux_path": "arduino-cli",
    "windows_path": "arduino-cli.exe",
    "flags": "compile -b STMicroelectronics:stm32:Blues -v --output-dir @{file_dir_name}/build"
  },
  "flasher": {
    "name": "arduino_cli",
    "linux_path": "arduino-cli",
    "windows_path": "arduino-cli.exe",
    "flags": "upload @{file_dir_name} -p @{port} -b STMicroelectronics:stm32:Blues"
  },
  "api_paths": [
    "ai_thinker/ai_wb2_32s_kit",
    "arduino/core",
    "arduino/uart",
    "arduino/i2c"
  ],
  "v_defines": [],
  "default_cpu_freq": 72000000,
  "default_string_len": 50,
  "compiler_setup_path": "setup/arduino32.json"
}
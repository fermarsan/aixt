{
    "target": "Nucleo_L031K6",
    "board": "Nucleo-L031K6",
    "backend": "arduino",
    "cc": {
        "name": "arduino_cli",
        "linux_path": "arduino-cli",
        "windows_path": "arduino-cli.exe",
        "flags": "compile -b STMicroelectronics:stm32:GenL0 -v --output-dir @{file_dir_name}/build"
    },
    "flasher": {
        "name": "arduino_cli",
        "linux_path": "arduino-cli",
        "windows_path": "arduino-cli.exe",
        "flags": "upload @{file_dir_name} -p @{target} -b STMicroelectronics:stm32:GenL0"
    },
    "api_paths": [
        "ST/Nucleo_L031K6",
        "Arduino/arduino_generic",
        "Arduino/arduino_generic_UART",
        "Arduino/arduino_generic_i2c"
    ],
    "v_defines": [],
    "default_cpu_freq": 32000000,
    "default_string_len": 50,
    "compiler_setup_path": "setup/arduino32.json"
}
// PIN functions (WCH-CH573F)

module pin

@[inline]
pub fn high_(port, ...any) {
	C.GPIO//##port##_SetBits(GPIO_Pin_##__VA_ARGS__)
}

@[inline]
pub fn high(id u8) {
	C.pin.high_(id u8)
}

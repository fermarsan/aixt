// PIN functions (WCH-CH582F)

module pin

@[inline]
pub fn toggle_(port, ...) {
	C.GPIO##port##_InverseBits(GPIO_Pin_##__VA_ARGS__)
}

@[inline]
pub fn toggle(id u8) {
	C.pin.toggle_(id u8)
}
